nership with Astrazeneca Plc. The company was formerly known as Bristol-Myers Company and changed its name to Bristol-Myers Squibb Company in 1989. Bristol-Myers Squibb Company was founded in 1887 and is headquartered in New York, New York.","city":"New York","phone":"212 546 4000","state":"NY","country":"United States","companyOfficers":[],"website":"http:\u002F\u002Fwww.bms.com","maxAge":86400,"address1":"430 East 29th Street","industry":"Drug Manufacturersâ€”General","address2":"14th Floor"},"recommendationTrend":{"trend":[{"period":"0m","strongBuy":5,"buy":4,"hold":11,"sell":2,"strongSell":0},{"period":"-1m","strongBuy":6,"buy":5,"hold":5,"sell":0,"strongSell":0},{"period":"-2m","strongBuy":6,"buy":5,"hold":4,"sell":0,"strongSell":0},{"period":"-3m","strongBuy":4,"buy":2,"hold":10,"sell":1,"strongSell":0}],"maxAge":86400},"financialsTemplate":{"code":"N","maxAge":1},"earnings":{"maxAge":86400,"earningsChart":{"quarterly":[{"date":"2Q2019","actual":{"raw":1.18,"fmt":"1.18"},"estimate":{"raw":1.06,"fmt":"1.06"}},{"date":"3Q2019","actual":{"raw":1.17,"fmt":"1.17"},"estimate":{"raw":1.07,"fmt":"1.07"}},{"date":"4Q2019","actual":{"raw":1.22,"fmt":"1.22"},"estimate":{"raw":0.88,"fmt":"0.88"}},{"date":"1Q2020","actual":{"raw":1.72,"fmt":"1.72"},"estimate":{"raw":1.49,"fmt":"1.49"}}],"currentQuarterEstimate":{"raw":1.5,"fmt":"1.50"},"currentQuarterEstimateDate":"2Q","currentQuarterEstimateYear":2020,"earningsDate":[{"raw":1595462400,"fmt":"2020-07-23"},{"raw":1595808000,"fmt":"2020-07-27"}]},"financialsChart":{"yearly":[{"date":2016,"revenue":{"raw":19427000000,"fmt":"19.43B","longFmt":"19,427,000,000"},"earnings":{"raw":4457000000,"fmt":"4.46B","longFmt":"4,457,000,000"}},{"date":2017,"revenue":{"raw":20776000000,"fmt":"20.78B","longFmt":"20,776,000,000"},"earnings":{"raw":1007000000,"fmt":"1.01B","longFmt":"1,007,000,000"}},{"date":2018,"revenue":{"raw":22561000000,"fmt":"22.56B","longFmt":"22,561,000,000"},"earnings":{"raw":4920000000,"fmt":"4.92B","longFmt":"4,920,0